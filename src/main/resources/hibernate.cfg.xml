<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="show_sql">false</property>
        <property name="hibernate.id.new_generator_mappings">false</property>
        <mapping class="model.events.ChildMark"/>
        <mapping class="model.events.FileResource"/>
        <mapping class="model.events.Lesson"/>
        <mapping class="model.events.LessonEvent"/>
        <mapping class="model.events.LessonEventType"/>
        <mapping class="model.events.Subject"/>
        <mapping class="model.security.Authority"/>
        <mapping class="model.security.User"/>
        <mapping class="model.security.User"/>
        <mapping class="model.users.Child"/>
        <mapping class="model.users.Parent"/>
        <mapping class="model.users.SchoolClass"/>
        <mapping class="model.users.Teacher"/>
    </session-factory>

</hibernate-configuration>